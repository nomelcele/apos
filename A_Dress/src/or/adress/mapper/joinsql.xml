<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="join">
	<insert id="login" parameterType="jvo">
		insert into login values(#{username},#{password},1)
	</insert>
	<insert id="grade" parameterType="jvo">
		insert into grade values(#{username},#{role})
	</insert>
</mapper>